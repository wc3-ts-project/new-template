{
    "name": "@write-coin/new-ceres-ts-template",
    "version": "1.0.0",
    "scripts": {
        "clear": "rm -rf target",
        "watch": "ceres-tstl --watch",
        "dev": "npm-watch",
        "build": "ceres-tstl",
        "build:defs": "ts-node scripts/dev",
        "lint": "eslint --ext ts ."
    },
    "license": "MIT",
    "devDependencies": {
        "@types/fs-extra": "9.0.12",
        "@types/node": "14.17.9",
        "@typescript-eslint/eslint-plugin": "2.19.0",
        "@typescript-eslint/parser": "2.19.0",
        "async": "^3.2.4",
        "ceres-decl": "1.0.0",
        "ceres-tstl": "0.31.0",
        "copyfiles": "2.2.0",
        "eslint": "6.8.0",
        "eslint-config-prettier": "6.10.0",
        "eslint-plugin-prettier": "3.4.0",
        "files": "1.1.0",
        "fs-extra": "9.0.1",
        "is-async-function": "^2.0.0",
        "lua-types": "2.8.0",
        "luamin": "1.0.4",
        "npm-watch": "0.7.0",
        "prettier": "1.19.1",
        "ts-node": "9.1.1",
        "tsc-watch": "4.4.0",
        "tsconfig": "^7.0.0",
        "typescript": "3.9.10",
        "typescript-to-lua": "0.31.0",
        "w3ts": "2.1.3",
        "war3tstlhelper": "1.0.1",
        "winston": "3.3.3"
    },
    "watch": {
        "build:defs": {
            "patterns": [
                "./maps/*"
            ],
            "extensions": "lua",
            "legacyWatch": true
        }
    },
    "dependencies": {
        "cerrie": "^1.0.4",
        "false": "^0.0.4",
        "new_package": "^1.0.0",
        "true": "^0.0.4",
        "war3-types": "1.0.4"
    },
    "description": "This repository has a configuration template for using Ceres with TypeScript.",
    "main": ".eslintrc.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wc3-ts-project/ceres-ts-template.git"
    },
    "author": "WriteCoin",
    "bugs": {
        "url": "https://github.com/wc3-ts-project/ceres-ts-template/issues"
    },
    "homepage": "https://github.com/wc3-ts-project/ceres-ts-template#readme"
}
